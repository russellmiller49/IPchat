    },
    {
      "Bounds": [
        44.95750427246094,
        458.78236389160156,
        547.0960388183594,
        497.9638671875
      ],
      "Font": {
        "alt_family_name": "Adv TTc 9c 3bd",
        "embedded": true,
        "encoding": "MacRomanEncoding",
        "family_name": "Adv TTc 9c 3bd 71",
        "font_type": "Type1",
