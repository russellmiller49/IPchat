        290.5244598388672,
        405.0634307861328
      ],
      "Font": {
        "alt_family_name": "Adv OT 21921 3d",
        "embedded": true,
        "encoding": "WinAnsiEncoding",
        "family_name": "Adv OT 21921 3d 4",
        "font_type": "Type1",
        "italic": false,
